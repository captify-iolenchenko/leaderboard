{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "env": {
    "development": {
      "presets": [
        ["es2015"],
        "react",
        "stage-0"
      ],
      "plugins": [
        "react-hot-loader/babel",
        "transform-react-jsx-source"
      ]
    }
  },
  "plugins": [
    "transform-class-properties",
    "lodash",
    "syntax-class-properties",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}
